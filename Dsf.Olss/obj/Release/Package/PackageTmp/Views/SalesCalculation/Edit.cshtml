@section Scripts{
    <script src="@Url.Content("~/Content/ui-framework/js/datatables/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Content/ui-framework/js/datepicker/bootstrap-datepicker.js")"></script>
    <script src="@Url.Content("~/Content/ui-framework/js/parsley/parsley.min.js")"></script>
    <script src="@Url.Content("~/Content/ui-framework/js/currencyformat/jquery.formatCurrency-1.4.0.js")"></script>
    <script src="@Url.Content("~/Content/js/Olss.js")"></script>
    <script src="@Url.Content("~/Content/ui-framework/js/bootbox/bootbox.min.js")"></script>
    <script src="~/Scripts/jquery.signalR-2.1.2.js"></script>
    <script src="~/signalr/hubs"></script>

    <script>
        $footer = $('.panel-footer');
        $button = $footer.find("[name='statusButton']");
        var SalesQuotationHub = $.connection.SalesQuotation;
        var SalesCalculationHub = $.connection.SalesCalculation;
        var confirm = false;

        SalesQuotationHub.client.OnStatusValidateCalcLock = function (id) {
            if (id == '@Model.IdCalculationOfSales' && !confirm) {
                confirm = true;
                bootbox.alert("Status of quotation has been changes (validated) by other user, you will be redirected to list page ..", function () {
                    window.location.href = '@Url.Action("Index", "SalesCalculation")';
                });
            }
        };

        SalesQuotationHub.client.OnStatusInvalidateCalcLock = function (id) {
            if (id == '@Model.IdCalculationOfSales' && !confirm) {
                confirm = true;
                bootbox.alert("Status of quotation has been changes (validated) by other user, you will be redirected to list page ..", function () {
                    window.location.href = '@Url.Action("Index", "SalesCalculation")';
                });
            }
        };

        SalesCalculationHub.client.OnStatusLock = function (id) {
            if (id == '@Model.IdCalculationOfSales' && !confirm) {
                confirm = true;
                bootbox.alert("Status has been changes by other user, you will be redirected to list page ..", function () {
                    window.location.href = '@Url.Action("Index", "SalesCalculation")';
                });
            }
        };

        SalesCalculationHub.client.OnSaveLock = function (id) {
            if (id == '@Model.IdCalculationOfSales' && !confirm) {
                confirm = true;
                bootbox.confirm("Data has been changes by other user, do you want to refresh the page?", function (result) {
                    if (result != false) {
                        location.reload();
                    }
                    confirm = false;
                });
            }
        };

        $.connection.hub.start().done(function () {
            var myClientId = $.connection.hub.id;
            $('#ConnId').val(myClientId);
        });


        function disableValue($object, $objectAmount) {
            var value = $object.val();
            if (value == '') {
                $objectAmount.val(0);
                $objectAmount.attr('readonly', 'readonly');
            }
            else {
                $objectAmount.val('');
                $objectAmount.removeAttr('readonly');
            }
            $('#TotalAmount').change();
        }

        $('#COSOtherExpense').change(function () {
