                                        @Html.TextBoxFor(o => Model.RepNamePositionModels[0].sbtYear, new { @class = "form-control tempData Name hidden", @type = "text", @Id = "sbtYear", @value = "" })
                                        @Html.TextBoxFor(o => Model.RepNamePositionModels[0].CreatedBy, new { @class = "form-control tempData Name hidden", @type = "text", @Id = "CreatedBy", @value = "" })
