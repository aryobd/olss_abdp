                                    </div>
                                    @{
    var selReportType = new List<SelectListItem>
                                            {
                                                  new SelectListItem { Text = "Pribadi", Value = "0" },
                                                  new SelectListItem { Text = "Badan Hukum", Value = "1" },
                                            };
                                    }
                                    @Html.DropDownListFor(o => o.RepNamePositionModels[0].ReportType, new SelectList(selReportType, "value", "Text", Model.RepNamePositionModels[0].ReportType), new { @class = "form-control", @type = "text", @onchange = "toggleDIvDisplay(this.value)", @id = "ReportType" })
                                </div>
                            </div>
                            @if (Model.RepNamePositionModels[0].ReportType == "1")
                            {
                                <script>
                                    $('#Name').show();
                                    $('#Position').show();
                                </script>
                            }
                            else
                            {
                                <script>
                                    $('#Name').hide();
                                    $('#Position').hide();
                                </script>
                            }
                            <div id="Name" class="row">
                                <div class="form-group">
                                    <label>Representative's Name</label>
                                    @Html.TextBoxFor(o => Model.RepNamePositionModels[0].RepName, new { @class = "form-control mandatory tempData Name", @type = "text", @Id = "RepName", @value = "" })
                                </div>
                            </div>
                            <div id="Position" class="row">
                                <div class="form-group">
                                    <label>Representative's Position</label>
                                    @Html.TextBoxFor(o => Model.RepNamePositionModels[0].RepPosition, new { @class = "form-control mandatory tempData Name", @type = "text", @Id = "RepPosition", @value = "" })
                                </div>
                            </div>
                            @if (Model.UpdateJualBeliModels != null)
                            {
                                <div class="row configuration-table">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th hidden="hidden" width="10%">IdTb_OPL_Unit</th>
                                                <th hidden="hidden" width="10%">CreatedBy</th>
                                                <th hidden="hidden" width="10%">CreatedDate</th>
                                                <th hidden="hidden" width="10%">IdPJB_BPKB</th>
                                                <th width="10%">Chassis</th>
                                                <th width="80%">BPKB Name</th>
                                            </tr>
                                        </thead>
                                        <tbody id="dg_AssetBPKBName">
                                            @if (Model != null)
                                            {
                                                if (Model.UpdateJualBeliModels != null)
                                                {
                                                    for (int i = 0; i < Model.UpdateJualBeliModels.Count; i++)
                                                    {
                                                        <tr>
                                                            <td hidden="hidden">@Html.TextBoxFor(o => Model.UpdateJualBeliModels[i].IdTb_OPL_Unit, new { @class = "form-control mandatory tempData Name hidden", @type = "text", @Id = "IdTb_OPL_Unit_" + i, @value = "" })</td>
                                                            <td>@Html.DisplayTextFor(o => Model.UpdateJualBeliModels[i].Chassis)</td>
                                                            <td>@Html.TextBoxFor(o => Model.UpdateJualBeliModels[i].BPKBName, new { @class = "form-control mandatory tempData Name", @Id = "BPKB_" + i, @value = "" })</td>
                                                            <td hidden="hidden">@Html.TextBoxFor(o => Model.UpdateJualBeliModels[i].CreatedBy, new { @class = "form-control mandatory tempData Name hidden", @type = "text", @Id = "CreatedBy" + i, @value = "" })</td>
                                                            <td hidden="hidden">@Html.TextBoxFor(o => Model.UpdateJualBeliModels[i].CreatedDate, new { @class = "form-control mandatory tempData Name hidden", @type = "text", @Id = "CreatedDate" + i, @value = "" })</td>
                                                            <td hidden="hidden">@Html.TextBoxFor(o => Model.UpdateJualBeliModels[i].IdPJB_BPKB, new { @class = "form-control mandatory tempData Name hidden", @type = "text", @Id = "IdPJB_BPKB" + i, @value = "" })</td>
                                                        </tr>
                                                    }
                                                }
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            }
                        </div>
                    </div>



                    <div class="modal-footer">
                        <input type="button" name="btnUpdateJualBeli" value="Save" class="btn btn-info" onclick="UpdateJualBeli()" />
                        <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>

                    </div>
                </div>
            </div>
            <div class="col-xs-3">

            </div>
        </div>
    </div>
}

<script type="text/javascript">
    function toggleDIvDisplay(e) {
        if (e == "1") {
            $('#Name').show();
            $('#Position').show();
        }
        else {
            $('#Name').hide();
            $('#Position').hide();
        }
    }
</script>

