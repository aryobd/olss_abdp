<div class="modal fade" id="PrepHistoryModal" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <section class="panel" style="margin-bottom: 0px;">
                <header class="panel-heading font-bold">
                    Unit Preparation Progress History
                    <i class="fa fa-info-sign text-muted" data-toggle="tooltip" data-placement="bottom" data-title="ajax to load the data."></i>
                </header>
                <div class="panel-body" style="padding: 0px !important;">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>DSF Office</label></div>
                                <div class="col-lg-8">: <label id="valDSFOffice"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>Agreement Number</label></div>
                                <div class="col-lg-8">: <label id="valAgreementNumber"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>Customer Name</label></div>
                                <div class="col-lg-8">: <label id="valCustomerName"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>PO Number</label></div>
                                <div class="col-lg-8">: <label id="valPONumber"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>PO Creation Date</label></div>
                                <div class="col-lg-8">: <label id="valPOCreationDate"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>PO Approved Date</label></div>
                                <div class="col-lg-8">: <label id="valPOApprovedDate"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>Supplier Name</label></div>
                                <div class="col-lg-8">: <label id="valSupplierName"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>Unit Description</label></div>
                                <div class="col-lg-8">: <label id="valUnitDescription"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>Unit Status</label></div>
                                <div class="col-lg-8">: <label id="valUnitStatus"></label></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="form-group">
                                <div class="col-lg-4"><label>Qty</label></div>
                                <div class="col-lg-8">: <label id="valQty"></label></div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <section class="panel" style="margin-bottom: 0px;">
                <div class="panel-body" style="padding: 0px 0px;">
                    <div class="DocumentList">
                        <div class="table-responsive history-responsive">
                            <table id="history-list" class="table table-striped m-b-none tablewrap">
                                <thead>
                                    <tr>
                                        <th>Update Date</th>
                                        <th>Progress Status</th>
                                        <th>Remark</th>
                                        <th>Unit Readiness</th>
                                        <th>Update By</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </section>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

